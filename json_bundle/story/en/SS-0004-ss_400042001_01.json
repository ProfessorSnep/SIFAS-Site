        {
            "command": "wait",
            "duration": 0.3
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Umi",
            "line": "Without the conveniences of civilization, all one can rely on is her own knowledge and experience. Nature shows no mercy to people like us who are used to modern living.",
            "voice_idx": "16",
            "is_plus": true
