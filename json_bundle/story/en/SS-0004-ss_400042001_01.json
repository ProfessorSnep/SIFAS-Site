        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "You really like the mountains, huh? What is it about them you enjoy so much?",
            "voice_idx": "14",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Umi",
            "line": "As I said the last time we talked about hiking, the mountainside is full of things you can't experience otherwise.",
            "voice_idx": "15",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "eye",
            "visual_eyes": "close"
