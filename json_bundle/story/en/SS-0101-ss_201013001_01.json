        },
        {
            "command": "set_label",
            "label_value": "aqours @chika"
        },
        {
            "command": "speak",
            "speaker": "Chika",
            "line": "Do you get that feeling too?",
            "voice_idx": "5",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Chika...",
