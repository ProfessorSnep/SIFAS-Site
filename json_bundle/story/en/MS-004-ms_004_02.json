        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Chika",
            "line": "Festivals are made by everyone. Festivals come to be from the love in everyone's hearts!",
            "voice_idx": "38",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
