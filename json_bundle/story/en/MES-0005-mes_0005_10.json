        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Rin",
            "line": "You think so?",
            "voice_idx": "8",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "They wouldn't have done that unless they were having fun.",
