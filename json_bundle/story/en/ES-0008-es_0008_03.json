        {
            "command": "character",
            "character_id": "11",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "No, I'm enrolled in the regular course, but I'm filling in for Kanata due to a sudden turn of events.",
