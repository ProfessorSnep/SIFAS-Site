            "visual_expression": "5"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Eli",
            "line": "What? Did something happen to her?",
            "voice_idx": "19",
