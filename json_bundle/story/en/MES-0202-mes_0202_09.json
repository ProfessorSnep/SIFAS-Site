        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Kasumi, you did a great job training today!",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "You too! Today's regimen was perfect for me!",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "I must have some incredible appeal if you're making me work on all of that advanced stuff!",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "unknown_select",
            "select_args": "Exactly. select_1"
        },
        {
            "command": "unknown_select",
            "select_args": "Huh? select_2"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Exactly. I'm totally gaga over you, Kasumi.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "5"
        },
        {
            "command": "character",
