        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "Oh, I love you, you totally get me! I know I'm adorable, but I'm trying to come up with a plan to get everyone to think I'm cuter than ever before!",
            "voice_idx": "3",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 1.0
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "stopshake",
            "character_args": []
