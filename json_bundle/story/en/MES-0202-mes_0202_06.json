        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "I have a lot of thoughts I wanna share about them, after all.",
            "voice_idx": "20",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
