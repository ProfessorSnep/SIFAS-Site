        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Great job, Shizuku! You were so good! This solo event was a huge success!",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "create"
        },
        {
            "command": "character",
            "character_id": "1",
