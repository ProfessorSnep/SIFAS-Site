            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "And you're a wonderful friend, coming out for Kasumi.",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Well, this is about all I can do.",
            "voice_idx": null,
