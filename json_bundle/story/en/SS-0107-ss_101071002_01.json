        },
        {
            "command": "speak",
            "speaker": "Hanamaru",
            "line": "I've been recommending my favorite books as part of the library committee, but I don't think I'm reaching too many people.",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "character",
