        },
        {
            "command": "speak",
            "speaker": "Hanamaru",
            "line": "I really think reading will enrich your life.",
            "voice_idx": "9",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
