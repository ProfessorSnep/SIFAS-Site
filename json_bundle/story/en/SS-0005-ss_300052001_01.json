        {
            "command": "speak",
            "speaker": "Rin",
            "line": "Work hard, eat hard! Kayo-chin, Maki, let's go out for ramen! I found a coupon in the newspaper today!",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
