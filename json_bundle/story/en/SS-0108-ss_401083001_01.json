        {
            "command": "speak",
            "speaker": "Mari",
            "line": "*Chuckle* I wanted everyone to enjoy themselves, so I pulled out all the stops.",
            "voice_idx": "11",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
