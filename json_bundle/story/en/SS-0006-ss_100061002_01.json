        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Maki",
            "line": "E-Er, that was just me talking to myself! It's nothing, don't worry about it.",
            "voice_idx": "9",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
