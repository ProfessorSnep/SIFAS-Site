            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Karin",
            "line": "Well, if you insist. I can't keep you waiting on an answer.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
