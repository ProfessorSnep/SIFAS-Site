        {
            "command": "speak",
            "speaker": "Karin",
            "line": "What I do on my days off? Oh, interested in me, huh? *Giggle*",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
