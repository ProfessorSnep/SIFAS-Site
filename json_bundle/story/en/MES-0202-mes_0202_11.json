        {
            "command": "speak",
            "speaker": "[player]",
            "line": "There you are, Kasumi.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "hide"
        },
        {
