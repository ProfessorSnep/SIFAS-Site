        },
        {
            "command": "wait",
            "duration": 0.3
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "Y-Yeah! *Giggle* Hearing you say it makes me so happy!",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "unknown_goto",
            "goto_args": "main_1"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I mean, you're you! The girl who loves school idols so much, at one point she managed a one-person club on her own. Of course everyone wants to cheer for you, me included!",
            "voice_idx": null,
            "is_plus": false
        },
        {
