        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "6"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Karin",
            "line": "What's gotten into you? Of course I do. We've gotten close, after all.",
            "voice_idx": "21",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Hahaha, you don't have to get all embarrassed.",
            "voice_idx": null,
            "is_plus": false
