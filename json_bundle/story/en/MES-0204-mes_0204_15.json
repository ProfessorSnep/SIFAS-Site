        },
        {
            "command": "speak",
            "speaker": "Karin",
            "line": "C'mon, why are you staring at me again?",
            "voice_idx": "20",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Oh, nothing. I was just thinking. You really love everyone in our club, huh?",
            "voice_idx": null,
