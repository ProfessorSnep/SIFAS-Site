        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Your expressiveness has hit a new level lately, Shizuku! Watching you practice again today got me feeling all excited!",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Shizuku",
            "line": "Oh, really?!",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "shake",
            "character_args": [
                "x",
                "2",
                "6"
            ]
        },
        {
            "command": "wait",
            "duration": 0.2
        },
        {
