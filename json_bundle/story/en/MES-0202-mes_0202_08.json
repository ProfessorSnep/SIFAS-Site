        },
        {
            "command": "speak",
            "speaker": "Ai",
            "line": "Rivals? In what way?",
            "voice_idx": "22",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "jump"
        },
        {
            "command": "wait",
