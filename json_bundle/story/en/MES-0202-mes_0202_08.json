        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Definitely. Plus, I think there are plenty of things only you can do, Kasumi.",
            "voice_idx": null,
            "is_plus": false
