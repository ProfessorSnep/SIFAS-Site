        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "I don't mind our club president helping, but Ayumu and Ai, you two can forget it! You're my rivals, after all.",
            "voice_idx": "21",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "5"
        },
        {
            "command": "character",
            "character_id": "2",
