        {
            "command": "speak",
            "speaker": "Ayumu",
            "line": "*Giggle* Personally, I think it's a great idea, Kasumi.",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
