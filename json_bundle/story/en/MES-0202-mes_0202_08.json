        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Ayumu",
            "line": "Let us know if there's anything we can do. We'd be happy to help you out!",
            "voice_idx": "20",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
