        {
            "command": "speak",
            "speaker": "[player]",
            "line": "The wind feels so nice! It's nice to do stuff like this every once in a while.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
