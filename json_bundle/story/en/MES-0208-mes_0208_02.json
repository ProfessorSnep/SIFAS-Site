        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "Yeah, it's like a secret date!",
            "voice_idx": "1",
            "is_plus": false
        },
        {
