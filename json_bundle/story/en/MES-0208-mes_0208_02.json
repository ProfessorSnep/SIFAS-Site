        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "*Giggle* Let's enjoy our time alone. Even if it is technically club activities.",
            "voice_idx": "2",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.1
        },
        {
            "command": "character",
