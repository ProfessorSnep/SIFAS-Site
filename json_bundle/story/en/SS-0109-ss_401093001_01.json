        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Wait, really? I mean, Kasumi told me that you were totally bold and fearless out there.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Ruby",
            "line": "Well, that's because my sister was there with me.",
            "voice_idx": "8",
            "is_plus": false
