        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Ruby",
            "line": "Well, what I'm after is this kimono magazine.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Wow, you're interested in kimonos? Wait. Ohhh, is it the one you modeled for?",
