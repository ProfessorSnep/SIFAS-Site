        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Then I say you write the lyrics, Ai.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "5"
