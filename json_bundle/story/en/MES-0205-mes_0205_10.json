        {
            "command": "speak",
            "speaker": "Ai",
            "line": "Whoa! Y-You scared me. Geez, do you have me bugged or something?",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Huh? Why do you say that?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
