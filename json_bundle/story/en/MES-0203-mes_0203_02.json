        },
        {
            "command": "speak",
            "speaker": "Shizuku",
            "line": "Yes! I think you would do a wonderful job! Is there any particular role you want to play? The father? The mother?",
            "voice_idx": "19",
            "is_plus": false
        },
        {
            "command": "unknown_goto",
