        {
            "command": "speak",
            "speaker": "[player]",
            "line": "If anyone hears you say that, you might end up in time-out, too.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "5"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Rin",
            "line": "Oh! I-I sure hope not! Please don't tell anyone!",
            "voice_idx": "13",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 3.2
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "end_delay"
        },
        {
            "command": "wait_for_click"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "*Laugh* You got it. Don't tell anyone I said it sounded fun either. It'll be our little secret!",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Rin",
            "line": "Okay. Just between us!",
            "voice_idx": "14",
            "is_plus": true
        },
