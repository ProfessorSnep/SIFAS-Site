        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kotori",
            "line": "Sure. I'm nervous about performing alone, but...",
            "voice_idx": "5",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "But what?",
            "voice_idx": null,
