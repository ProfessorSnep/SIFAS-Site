        {
            "command": "speak",
            "speaker": "Nozomi",
            "line": "What's the matter, Eli-chi?",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "eye",
