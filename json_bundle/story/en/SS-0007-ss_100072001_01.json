        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Nozomi",
            "line": "Yeah! She may not think she's too useful, but...",
            "voice_idx": "15",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "eye",
            "visual_eyes": "close"
