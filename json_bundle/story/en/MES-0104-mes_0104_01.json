        {
            "command": "speak",
            "speaker": "Dia",
            "line": "Yes? Oh, good day! Out for a stroll?",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Yeah, what are you up to? I see a bunch of students around.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "Just as you can see, we're collecting garbage. It is cleanliness month at school, so we're all trying to clean up the town.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I see.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "Have you finished all the juice in that can you are holding?",
            "voice_idx": "3",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Huh? Oh yeah, I just finished.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "hide"
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "create"
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "visual",
            "visual_character": "104",
            "visual_outfit": "1"
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "scale",
            "scale_x": 1.2,
            "scale_y": 1.2
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "pos",
            "pos_x": "0",
            "pos_y": "-40"
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "pos",
            "pos_x": "/0",
            "pos_y": "/-45"
        },
        {
            "command": "character",
