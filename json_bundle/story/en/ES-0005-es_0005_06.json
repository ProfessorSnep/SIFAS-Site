        },
        {
            "command": "speak",
            "speaker": "Chika",
            "line": "It's like you know just what to do to help me relax. You're some sorta hugging expert!",
            "voice_idx": "16",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
