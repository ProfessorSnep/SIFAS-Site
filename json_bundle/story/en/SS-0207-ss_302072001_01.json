        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "visual",
            "visual_character": "201",
            "visual_outfit": "43001"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "6"
        },
        {
            "command": "character",
            "character_id": "2",
