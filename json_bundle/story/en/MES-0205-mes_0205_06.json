        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Phew! I think that'll be enough for everyone. I'm glad we worked so hard every day.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
