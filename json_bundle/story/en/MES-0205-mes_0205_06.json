        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Ai",
            "line": "Geez, now I can't stop thinking about it! I wanna practice some camp songs! You wanna hear?",
            "voice_idx": "19",
            "is_plus": true
