        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "Kanan, you know that there are other toppings, right? Roasted pork, bamboo shoots, green onion, marinated eggs... You can even make your own toppings, if you so choose.",
            "voice_idx": "14",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "4"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanan",
            "line": "My own, original toppings, huh...",
            "voice_idx": "15",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
