        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "At the diving shop? Well, I suppose it wouldn't be too strange to serve a light meal like that.",
            "voice_idx": "4",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 3.0
