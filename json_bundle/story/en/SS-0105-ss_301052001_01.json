        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Chika",
            "line": "Yeah, I thought the same thing when Honoka told me!",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "You",
            "line": "I'm really glad you came to me, too.",
            "voice_idx": "5",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "5"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Chika",
            "line": "I'm always coming to you, you know? I don't know how many times you've bailed me out of something!",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "set_viewing",
