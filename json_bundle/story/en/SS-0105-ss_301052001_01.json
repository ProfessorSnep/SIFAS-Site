        },
        {
            "command": "speak",
            "speaker": "You",
            "line": "A surprise party sounds pretty cool, and it's exciting just thinking about prepping for it without everyone else knowing!",
            "voice_idx": "3",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
