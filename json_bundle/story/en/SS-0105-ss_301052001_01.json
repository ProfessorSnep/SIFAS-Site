        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "You",
            "line": "No need to apologize. I just wanted us to do something together, that's all.",
            "voice_idx": "9",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.6
        },
        {
            "command": "character",
