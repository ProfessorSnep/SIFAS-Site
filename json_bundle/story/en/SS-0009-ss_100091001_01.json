        },
        {
            "command": "speak",
            "speaker": "Nico",
            "line": "Hm? What was that? It's how I spend my days of, of course.",
            "voice_idx": "9",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
