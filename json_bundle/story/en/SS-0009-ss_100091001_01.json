        },
        {
            "command": "speak",
            "speaker": "Nico",
            "line": "What, you want to know how I really spend them?",
            "voice_idx": "10",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "4"
