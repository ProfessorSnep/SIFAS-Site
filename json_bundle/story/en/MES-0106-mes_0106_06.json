        {
            "command": "speak",
            "speaker": "Yoshiko",
            "line": "Geez, it's about time! What took you so long? I told you to come right away!",
            "voice_idx": "8",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.2
