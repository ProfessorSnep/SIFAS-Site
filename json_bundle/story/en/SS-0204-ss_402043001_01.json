        {
            "command": "speak",
            "speaker": "Karin",
            "line": "Hey, what would you do if I were a princess?",
            "voice_idx": "9",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Karin",
            "line": "I'd live as a princess in one of those huge castles with tons of history. Ooh, and I might even attend a coronation, too!",
            "voice_idx": "10",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "If you were a princess?",
            "voice_idx": null,
