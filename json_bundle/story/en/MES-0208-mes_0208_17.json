        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "Yeah, I can't stop smiling at the pictures we took last time!",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Haha. Can I see them too?",
