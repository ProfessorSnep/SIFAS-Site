        {
            "command": "speak",
            "speaker": "Kanan",
            "line": "What about your group at Nijigasaki? Did you all first meet in high school?",
            "voice_idx": "19",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
