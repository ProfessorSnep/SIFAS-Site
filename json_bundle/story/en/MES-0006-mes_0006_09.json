        },
        {
            "command": "speak",
            "speaker": "Maki",
            "line": "The people who come to solo live shows don't care about the results of our practice. They probably just want to have a good time.",
            "voice_idx": "19",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "eye",
            "visual_eyes": "open"
