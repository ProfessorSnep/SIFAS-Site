        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I feel really happy when I hear you playing the piano.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Oh! Was that response a little too heavy?",
            "voice_idx": null,
