        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "Plus, she's reminded me that I need to grow up, too. If I don't learn to let her go, she'll never take me seriously.",
            "voice_idx": "14",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
            "visual_expression": "2"
