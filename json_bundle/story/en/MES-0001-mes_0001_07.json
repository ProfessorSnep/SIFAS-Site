        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Honoka",
            "line": "What?! Me, do a solo show?! Wouldn't it be worrying if it was just me?",
            "voice_idx": "15",
            "is_plus": false
