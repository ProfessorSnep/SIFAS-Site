        {
            "command": "speak",
            "speaker": "[player]",
            "line": "*Laugh* That's definitely something Honoka would do.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Shizuku",
            "line": "But hearing about Honoka's method for getting into character has made me realize something. I understand the mistake I've been making.",
            "voice_idx": "31",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "What do you mean?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "eye",
            "visual_eyes": "close"
        },
        {
