        {
            "command": "speak",
            "speaker": "Shizuku",
            "line": "I think I understand how just one thing can lead the way for the rest of your life, though.",
            "voice_idx": "11",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Shizuku",
            "line": "There was a single play that drove me to become an actress.",
            "voice_idx": "12",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
