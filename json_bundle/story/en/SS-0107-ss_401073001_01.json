        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Hanamaru, how was your hike with Umi?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
