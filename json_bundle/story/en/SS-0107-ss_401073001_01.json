        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Wow, that sounds really cool. Who knew places like that really existed?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
