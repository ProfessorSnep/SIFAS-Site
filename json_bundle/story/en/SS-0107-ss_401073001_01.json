        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Hanamaru",
            "line": "Yup. I went home with a lot of valuable experiences.",
            "voice_idx": "5",
            "is_plus": false
        },
        {
            "command": "unknown_goto",
            "goto_args": "main_1"
