        },
        {
            "command": "speak",
            "speaker": "Riko",
            "line": "Is that so? Your seat is next to mine. What an incredible coincidence.",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Riko, do you come to concerts like this often?",
