        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Karin",
            "line": "I have experience with that, so I thought I'd try to share some pointers with everyone.",
            "voice_idx": "5",
            "is_plus": false
