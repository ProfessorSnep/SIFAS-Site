        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "Listen! I need your advice on something!",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Wh-What's wrong, Kasumi?! You've got terrible bags under your eyes...",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "eye",
            "visual_eyes": "close"
        },
        {
            "command": "wait",
            "duration": 0.3
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "shake",
            "character_args": [
                "x",
                "2",
                "6"
            ]
        },
        {
