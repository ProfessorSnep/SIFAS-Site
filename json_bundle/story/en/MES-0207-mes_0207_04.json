        },
        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "Thank you!",
            "voice_idx": "17",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "By the way, is there any particular reason you chose the name Setsuna Yuki?",
