        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "Like in this scene, all the characters unite and strengthen their bond, like they're friends forever! Even if they're separated they'll find each other.",
            "voice_idx": "13",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Yeah.",
            "voice_idx": null,
