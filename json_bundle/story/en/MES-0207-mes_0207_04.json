            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "Yeah, it's from a light novel I love! I feel like I gain a lot of power from using my favorite character's name.",
            "voice_idx": "18",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
