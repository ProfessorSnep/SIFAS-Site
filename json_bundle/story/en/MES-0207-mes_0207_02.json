        },
        {
            "command": "speak",
            "speaker": "Nana",
            "line": "You might find it strange, but I can only keep doing the school idol club because I do the student council.",
            "voice_idx": "18",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Really?",
            "voice_idx": null,
