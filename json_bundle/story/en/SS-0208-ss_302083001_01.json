        {
            "command": "speak",
            "speaker": "[player]",
            "line": "From the library? Oh, it's a Japanese book.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "Yup. It's a book of fairy tales written in Japanese. Long novels are still too hard for me, but even I can read fairy tales.",
            "voice_idx": "3",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.1
        },
        {
