        {
            "command": "wait_for_click"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "I want to read lots of books and learn even more Japanese words.",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "You're so studious, Emma. Your Japanese is already really good, but you're still striving to improve.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "*Giggle* Thank you. Oh, I should look for a new book to read. I'm almost done with this one.",
            "voice_idx": "5",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 2.8
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
