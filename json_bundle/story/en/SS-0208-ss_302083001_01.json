        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I feel like you'd probably get distracted and make a lot of stops on the way over. Even without the wolf getting in the way.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "shake",
            "character_args": [
                "x",
                "2",
                "5"
            ]
