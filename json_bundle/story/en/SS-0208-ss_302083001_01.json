        {
            "command": "wait",
            "duration": 0.2
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "I wouldn't do that if my grandma was waiting for me. I'd just make all those stops on the way back home.",
            "voice_idx": "12",
            "is_plus": true
        },
        {
            "command": "delay",
