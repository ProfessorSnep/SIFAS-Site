        },
        {
            "command": "wait",
            "duration": 0.2
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "Eep, you're embarrassing me. *Giggle* That makes me happy, though. Thank you!",
            "voice_idx": "10",
