            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Hanayo",
            "line": "Well, to me, becoming a school idol was pushing myself to begin with.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "eye",
            "visual_eyes": "close"
