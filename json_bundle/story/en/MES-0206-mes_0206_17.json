        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "I know it's tasty, but you gotta think about nutrition, too.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I know, but I kinda just eat whatever I'm in the mood for...",
            "voice_idx": null,
            "is_plus": false
