        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Okay then, don't mind if I do.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
