        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "You gotta be more mindful of your health. I'll let you have some of my salad, so please, eat up.",
            "voice_idx": "3",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "You sure I can have it?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "Of course I am. Go on!",
            "voice_idx": "4",
            "is_plus": true
