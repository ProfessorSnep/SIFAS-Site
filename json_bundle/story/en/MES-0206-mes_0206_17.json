        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "What?! You're having bread for lunch AGAIN?",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "4"
