        {
            "command": "speak",
            "speaker": "Shizuku",
            "line": "Yes. Actually... Would you be my script reading partner?",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Script reading partner? M-Me?",
