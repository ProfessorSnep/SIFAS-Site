        },
        {
            "command": "speak",
            "speaker": "Honoka",
            "line": "Huh? Nozomi, what's up with that chair in your hands?",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
