        {
            "command": "speak",
            "speaker": "[player]",
            "line": "You were never actually planning on going sightseeing in the first place, were you?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Mari",
            "line": "That's right. I lied. Sorry for tricking you.",
            "voice_idx": "9",
            "is_plus": false
        },
        {
            "command": "speak",
