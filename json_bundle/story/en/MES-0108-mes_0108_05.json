        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Mari",
            "line": "That's right! I'm here as the chairwoman of Uranohoshi. This wasn't just pleasure!",
            "voice_idx": "12",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Wait, seriously?! If you were working, wasn't I just getting in the way?!",
            "voice_idx": null,
