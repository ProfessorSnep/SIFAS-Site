        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Mari",
            "line": "What do you mean?",
            "voice_idx": "8",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "We only went to schools today, right? On top of that, you already had permission to go to them.",
            "voice_idx": null,
            "is_plus": false
