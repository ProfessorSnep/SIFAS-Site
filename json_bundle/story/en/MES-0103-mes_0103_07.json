        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Mari",
            "line": "Hee hee hee. The combination of diving suits and sashimi really tickled the curiosity of your viewers!",
            "voice_idx": "7",
            "is_plus": true
        },
        {
