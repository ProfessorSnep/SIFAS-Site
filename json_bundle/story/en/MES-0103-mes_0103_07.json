        },
        {
            "command": "speak",
            "speaker": "Kanan",
            "line": "Ugh. I mean, I can laugh about it now, but it was so embarrassing at the time!",
            "voice_idx": "8",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
