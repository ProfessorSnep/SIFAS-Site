        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "I understand Mari's approach, however, it would not be wise to merely be unconventional. Novelty alone will end in a temporary spike in popularity.",
            "voice_idx": "14",
            "is_plus": false
