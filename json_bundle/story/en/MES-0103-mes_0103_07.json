        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Mari",
            "line": "You definitely need some kind of impact to set you apart from the others! Don't be afraid to be a little unorthodox. Give it a shot!",
            "voice_idx": "13",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
