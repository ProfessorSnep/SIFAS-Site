        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Everyone! It's almost time to go home! Wait... Karin, are you the only one here for after school practice?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "music",
            "fade": true,
            "args": "in 1.5"
        },
        {
