        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "Not at all! I think I spent a total of about 100 yen for all of my ingredients.",
            "voice_idx": "7",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.5
