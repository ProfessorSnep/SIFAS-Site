            "character_args": [
                "y",
                "5",
                "2"
            ]
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "*Giggle* Surprising, is it? Me talking so big?",
            "voice_idx": "12",
            "is_plus": true
        },
