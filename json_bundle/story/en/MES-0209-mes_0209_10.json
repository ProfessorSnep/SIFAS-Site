            "character_id": "2",
            "character_cmd": "create"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "visual",
            "visual_character": "9999",
            "visual_outfit": "1"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "6"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "scale",
            "scale_x": 1.2,
            "scale_y": 1.2
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "pos",
            "pos_x": "0",
            "pos_y": "-40"
        },
        {
            "command": "character",
