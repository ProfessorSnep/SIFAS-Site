        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Do you know why that happened? Would you be okay talking about it?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "hide"
        },
        {
