        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "It's almost like being at camp or something.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "Yeah. I'm happy you think it's fun too.",
            "voice_idx": "4",
            "is_plus": false
        },
        {
