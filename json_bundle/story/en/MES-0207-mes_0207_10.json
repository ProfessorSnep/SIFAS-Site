        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Now we get to talk about school idols as much as we want.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
