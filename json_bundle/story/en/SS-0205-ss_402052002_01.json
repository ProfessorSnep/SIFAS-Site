            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "So I want to try to help her unmuddle those feelings, however I can.",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "2"
