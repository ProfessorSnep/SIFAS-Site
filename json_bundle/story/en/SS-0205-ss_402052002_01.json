        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "See, Kanan's writing some lyrics right now, but she's having a hard time figuring out what to write. Her feelings are just too muddled up.",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
