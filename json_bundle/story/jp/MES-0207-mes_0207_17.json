        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "せつ菜",
            "line": "だから、なんというか、枠……みたいなものは、<br/>今は作りたくないと思っています",
