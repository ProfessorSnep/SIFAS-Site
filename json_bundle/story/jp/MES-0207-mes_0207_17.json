        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "せつ菜",
            "line": "ファンクラブを作ってしまうと、どうしても入ってくれた人を<br/>優先してしまうと思うんです",
