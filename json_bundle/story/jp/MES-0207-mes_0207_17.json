        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "せつ菜",
            "line": "ファンクラブに入ってくれた人もそれを望むでしょうし、<br/>私自身その気持ちに応えたいと全力を尽くすと思います",
            "voice_idx": "10",
            "is_plus": false
