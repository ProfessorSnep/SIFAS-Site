        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "～～～～っ！<br/>もう1曲！　もう1曲だけ弾いて～っ！",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
