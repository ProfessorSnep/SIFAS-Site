        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "ダイヤ",
            "line": "きっと、凛さんなりに気を遣っていたのでしょうね。<br/>みなさんを心配させないように",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "うん。でも、元気になったなら良かったよ。<br/>体調を崩したっていう店長さんも、元気になったんでしょ？",
            "voice_idx": null,
            "is_plus": false
