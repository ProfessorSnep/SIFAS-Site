        },
        {
            "command": "speak",
            "speaker": "エマ",
            "line": "でも、見てたらうっとりしちゃって……。<br/>結局はじーっと見ちゃってるかな、えへへ",
            "voice_idx": "5",
            "is_plus": false
        },
