        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "エマ",
            "line": "曲を聞いて歌を覚えたり、<br/>ライブのDVDを見て、ダンスを真似してみたり……",
            "voice_idx": "4",
            "is_plus": false
        },
