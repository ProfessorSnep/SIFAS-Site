        },
        {
            "command": "speak",
            "speaker": "エマ",
            "line": "え、どうして知ってるの？",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "知ってる、ってなんのこと？",
