        {
            "command": "speak",
            "speaker": "エマ",
            "line": "うふふ、ありがとう♪<br/>だけど、それはきっと着物のおかげだね。<br/>それと、カメラマンさんが上手だから",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "私は、エマさんが着物を好き、っていう気持ちが<br/>表れてるからだと思うな",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
