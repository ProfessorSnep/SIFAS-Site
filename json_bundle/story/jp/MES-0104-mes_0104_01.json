        {
            "command": "speak",
            "speaker": "ダイヤ",
            "line": "……失礼しました。お気になさらないで",
            "voice_idx": "11",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "う、うん。大変だね、他の生徒を気に掛けながらの作業なんて",
            "voice_idx": null,
