        },
        {
            "command": "unknown_select",
            "select_args": "ごめんなさい…… select_1"
        },
        {
            "command": "unknown_select",
            "select_args": "すぐ飲んじゃうつもりだった select_2"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "あはは、ごめんなさい……。以後気を付けます",
            "voice_idx": null,
