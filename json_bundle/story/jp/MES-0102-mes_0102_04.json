        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "梨子",
            "line": "うん。歌っている人のイメージをメロディに反映するのも<br/>大事だもの。自然な姿を見たかったから、<br/>質問は控えようと思ったけど……",
            "voice_idx": "7",
            "is_plus": true
