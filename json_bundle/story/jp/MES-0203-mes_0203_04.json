        {
            "command": "speak",
            "speaker": "しずく",
            "line": "はああ……",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "しずくちゃん、元気出して。<br/>果林さんも悪気があったわけじゃないと思うし……<br/>多分……",
            "voice_idx": null,
