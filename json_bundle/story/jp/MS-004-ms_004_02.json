        },
        {
            "command": "wait_for_click"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "そういえば、初めて会った時も、曲作りに行き詰ってるって<br/>言ってたね……",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "千歌",
            "line": "お祭りで歌う曲だから、お祭りの賑やかさとか、わくわくする感じとかを出した曲にしたいと思って、歌詞を書いたんだ",
            "voice_idx": "27",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
