        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "千歌",
            "line": "今回は最後まで書き上げても、<br/>あんまりそういう感じにならなくて……<br/>でも、だからってこの歌詞が全然違うっていうわけでもなくて……",
            "voice_idx": "30",
