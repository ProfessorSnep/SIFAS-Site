        },
        {
            "command": "speak",
            "speaker": "ルビィ",
            "line": "こんにちは。調子はすごくいいよ。<br/>ダンスの練習、あなたが見てくれてるおかげで、<br/>自分でも上達してるのが分かるんだ",
            "voice_idx": "1",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.2
        },
        {
            "command": "character",
            "character_id": "1",
