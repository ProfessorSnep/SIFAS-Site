        {
            "command": "speak",
            "speaker": "善子",
            "line": "ねぇ、どうすればいい！？",
            "voice_idx": "11",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "どうって、参加したいならすればいいんじゃないの？",
