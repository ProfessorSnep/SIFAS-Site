        },
        {
            "command": "speak",
            "speaker": "愛",
            "line": "でしょ！？",
            "voice_idx": "14",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "愛ちゃんのダジャレって、周りを元気づける力があるよね。<br/>いや、愛ちゃんが言うからダジャレにそういう力が宿るのかな",
            "voice_idx": null,
            "is_plus": false
