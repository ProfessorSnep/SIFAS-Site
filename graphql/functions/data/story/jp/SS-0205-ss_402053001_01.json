        {
            "command": "speak",
            "speaker": "愛",
            "line": "ヘイ！　鮮魚市場に一番乗り～♪<br/>……じゃなかった！",
            "voice_idx": "3",
            "is_plus": true
        },
        {
            "command": "delay",
