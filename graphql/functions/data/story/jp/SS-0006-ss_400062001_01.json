        },
        {
            "command": "speak",
            "speaker": "しずく",
            "line": "滑る時、とっても楽しそうに<br/>声を上げていらしたじゃありませんか♪",
            "voice_idx": "11",
            "is_plus": false
        },
        {
            "command": "character",
