        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "真姫",
            "line": "生憎、この日は別の予定が入ってるの。<br/>ただなんとなく、この前行った水上アスレチックの<br/>イベントを思い出したから見てただけ",
            "voice_idx": "7",
            "is_plus": true
        },
