        },
        {
            "command": "speak",
            "speaker": "千歌",
            "line": "1人で歌うのがどんな感じなのか知りたくて、<br/>ライブを企画して、みんなにも家族にも協力してもらって、<br/>曲も今日のために頑張って作って……",
            "voice_idx": "9",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "9",
            "character_cmd": "face",
            "visual_expression": "4"
