        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "千歌",
            "line": "今日泊まりに来てくれてる人の中にもいるんだよ。<br/>ほら、ネットでもライブのこと、告知したじゃない？<br/>今日のためにわざわざ来てくれたんだって！",
            "voice_idx": "3",
