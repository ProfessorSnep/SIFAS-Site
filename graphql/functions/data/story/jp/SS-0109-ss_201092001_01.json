        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "ルビィ",
            "line": "衣装に付けるリボンだよ",
            "voice_idx": "3",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "リボン？",
            "voice_idx": null,
            "is_plus": false
