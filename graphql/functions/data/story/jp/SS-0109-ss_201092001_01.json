        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "ルビィ",
            "line": "今日買う材料で作る衣装も、リボンを使う予定なの。<br/>でも、どれにするかなかなか決められなくて……",
            "voice_idx": "5",
            "is_plus": false
