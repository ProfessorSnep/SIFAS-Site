        {
            "command": "end_delay"
        },
        {
            "command": "wait_for_click"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "へえ～。ほんとだ、サテンのとか、レースが付いてるのとか……<br/>あっ、これかわいい！　水玉のリボンだ！",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
