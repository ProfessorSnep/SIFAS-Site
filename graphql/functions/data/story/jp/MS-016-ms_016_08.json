        },
        {
            "command": "audio_effect",
            "effect_target": "voice",
            "effect_value": "AisacC12_Stage",
            "effect_param": "0.8"
        },
        {
            "command": "speak",
            "speaker": "歩夢",
            "line": "最初は出来なかったステップができるようになったり、<br/>前よりももっと声が出るようになったり、そのことがとても<br/>嬉しくて、気付いたら練習が楽しくて仕方なくなっていました",
            "voice_idx": "22",
            "is_plus": false
        },
        {
            "command": "set_label",
            "label_value": "@ayumu"
        },
        {
            "command": "audio_effect",
            "effect_target": "voice",
            "effect_value": "AisacC12_Stage",
            "effect_param": "0.8"
        },
        {
            "command": "speak",
            "speaker": "歩夢",
            "line": "練習を頑張れば、できることも増えていって、あの子もその度に<br/>喜んでくれて……。それになにより、ライブを見に来てくれた人が<br/>楽しいって言ってくれて……本当に幸せでした……",
            "voice_idx": "23",
            "is_plus": false
