        },
        {
            "command": "audio_effect",
            "effect_target": "voice",
            "effect_value": "AisacC12_Stage",
            "effect_param": "0.8"
        },
        {
            "command": "speak",
            "speaker": "歩夢",
            "line": "そして活動を重ねるにつれ、夢がなかった私にも、<br/>叶えたい夢ができました",
            "voice_idx": "25",
            "is_plus": false
