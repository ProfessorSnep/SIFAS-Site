            "label_value": "@ayumu"
        },
        {
            "command": "audio_effect",
            "effect_target": "voice",
            "effect_value": "AisacC12_Stage",
            "effect_param": "0.8"
        },
        {
            "command": "speak",
            "speaker": "歩夢",
            "line": "それをみんなと一緒に形にしたくて……。<br/>みんなで同じ目標に向かって努力するのが、<br/>楽しくて楽しくて仕方がないんです",
            "voice_idx": "26",
            "is_plus": false
        },
        {
            "command": "set_label",
            "label_value": "@ayumu"
