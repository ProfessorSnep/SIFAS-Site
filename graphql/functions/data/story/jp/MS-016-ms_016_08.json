        {
            "command": "set_label",
            "label_value": "@ayumu"
        },
        {
            "command": "audio_effect",
            "effect_target": "voice",
            "effect_value": "AisacC12_Stage",
            "effect_param": "0.8"
        },
        {
            "command": "speak",
            "speaker": "歩夢",
            "line": "努力する度、自分を含めて、みんなに幸せが広がっていくのが<br/>気持ちよくて。軽い気持ちで始めたスクールアイドルに、<br/>いつの間にか夢中になっていたんです",
            "voice_idx": "24",
            "is_plus": false
