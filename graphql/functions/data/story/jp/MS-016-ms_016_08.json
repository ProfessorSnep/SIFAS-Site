            "effect_param": "0.8"
        },
        {
            "command": "speak",
            "speaker": "歩夢",
            "line": "だけど、毎日練習している私を見て、あの子が嬉しそうに<br/>してくれて……その笑顔が、私、とっても好きで……<br/>またその顔を見たいな、って頑張るようになって……",
            "voice_idx": "21",
            "is_plus": false
        },
        {
            "command": "unknown_cg",
            "cg_arg": "3 hide 0.4"
        },
        {
            "command": "wait",
            "duration": 0.6
        },
        {
