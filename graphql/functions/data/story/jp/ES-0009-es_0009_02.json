        {
            "command": "set_label",
            "label_value": "@kasumi"
        },
        {
            "command": "speak",
            "speaker": "かすみ",
            "line": "絵里先輩ってお花にも詳しいんだ。<br/>私も何か見つけなくちゃ……あ、これ！",
