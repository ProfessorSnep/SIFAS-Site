        {
            "command": "speak",
            "speaker": "歩夢",
            "line": "ねえ、お願いしたいことがあるんだけど、今いいかな？",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "どうしたの、歩夢ちゃん？",
