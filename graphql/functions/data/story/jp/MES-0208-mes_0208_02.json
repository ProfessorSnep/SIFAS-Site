        {
            "command": "speak",
            "speaker": "[player]",
            "line": "あはは！　英才教育だ！",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
