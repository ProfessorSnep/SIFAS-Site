        },
        {
            "command": "speak",
            "speaker": "しずく",
            "line": "そ、そうでしょうか……",
            "voice_idx": "8",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "普段はしっかりしてるけど、<br/>かすみちゃんや璃奈ちゃんと一緒にいるときは年相応で、<br/>本当にかわいいんだよね",
            "voice_idx": null,
            "is_plus": false
        },
