        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "璃奈",
            "line": "でも、みんなそういうことは知らないから<br/>スクールアイドルをやるための道具だって思う人もいるんだよね",
            "voice_idx": "6",
            "is_plus": false
