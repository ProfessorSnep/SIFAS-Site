        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Eli",
            "line": "She reminded me how fun it is to pour your feelings into making something for someone. It's the same for both baking and school idol activities.",
            "voice_idx": "9",
            "is_plus": true
