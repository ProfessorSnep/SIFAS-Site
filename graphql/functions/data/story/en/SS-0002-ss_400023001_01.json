        {
            "command": "speak",
            "speaker": "Eli",
            "line": "Oh! I was actually reminded of something when I saw Setsuna put her heart and soul into baking.",
            "voice_idx": "8",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "eye",
            "visual_eyes": "close"
        },
        {
            "command": "wait",
            "duration": 0.3
