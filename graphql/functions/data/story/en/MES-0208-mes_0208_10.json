        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Sorry to bother you after practice, but I wanted to talk about the next song.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
