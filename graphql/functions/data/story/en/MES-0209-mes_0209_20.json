        {
            "command": "speak",
            "speaker": "Rina",
            "line": "I've been doing a lot of thinking since the other day.",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Mm-hmm.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
