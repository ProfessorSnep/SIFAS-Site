        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Nozomi",
            "line": "I feel like I can pull it off as long as you're around. You have my seal of approval!",
            "voice_idx": "10",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "*Chuckle* Thanks!",
