        },
        {
            "command": "speak",
            "speaker": "Yoshiko",
            "line": "I'm Yohane, not Yoshiko!",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "What are you three up to?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
