        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanan",
            "line": "And it would be so cool if we could dance in the water with the fish and the dolphins.",
            "voice_idx": "4",
            "is_plus": false
