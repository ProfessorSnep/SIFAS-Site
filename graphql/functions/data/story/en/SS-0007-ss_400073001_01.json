        {
            "command": "wait",
            "duration": 0.2
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Nozomi",
            "line": "Hehe, that's such a \"you\" response. But that's a bit different from what I talk about when stargazing.",
            "voice_idx": "5",
            "is_plus": true
        },
        {
            "command": "delay",
