        },
        {
            "command": "speak",
            "speaker": "Rin",
            "line": "*Munch* *Munch* *Gulp* Even though they're the same recipe?",
            "voice_idx": "19",
            "is_plus": false
        },
        {
            "command": "unknown_select",
            "select_args": "||I guess they taste different depending on who makes them.|| select_1"
