        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "And that's exactly what I'm angling for!",
            "voice_idx": "19",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "I want to write about what I usually keep hidden and other things like that so that my fans discover what I'm really like.",
            "voice_idx": "20",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 3.0
