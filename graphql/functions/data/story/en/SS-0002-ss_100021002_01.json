        {
            "command": "speak",
            "speaker": "Eli",
            "line": "What I want to challenge myself to do as a school idol?",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
