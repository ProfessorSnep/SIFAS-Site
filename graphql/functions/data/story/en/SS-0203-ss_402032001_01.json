        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "By the way, your water fountain timing was spot-on the other day, Shizuko. How did you do that?",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Shizuku",
            "line": "It was just a coincidence.",
            "voice_idx": "3",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "5"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "Really? It seemed too perfect to be coincidence.",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
