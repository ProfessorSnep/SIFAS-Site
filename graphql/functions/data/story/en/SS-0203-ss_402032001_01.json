        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "Seriously?! You're such a great actress! It looked like real magic! I-I mean, I could've easily pulled that off, too.",
            "voice_idx": "6",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 6.0
        },
        {
            "command": "character",
            "character_id": "2",
