            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Shizuku",
            "line": "I'm glad it looked that way. I tried my best to improvise so that it'd seem natural.",
            "voice_idx": "5",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "5"
        },
        {
            "command": "character",
