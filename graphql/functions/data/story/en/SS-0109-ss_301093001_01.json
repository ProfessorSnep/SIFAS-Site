            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Ruby",
            "line": "I'm so excited! I wish it were here already!",
            "voice_idx": "2",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.8
        },
        {
            "command": "character",
