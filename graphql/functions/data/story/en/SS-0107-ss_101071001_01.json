            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Hanamaru",
            "line": "When I clean the place, it really warms me up and makes me feel like I can take on the day!",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
