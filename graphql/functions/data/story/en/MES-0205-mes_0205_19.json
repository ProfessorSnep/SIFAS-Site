        },
        {
            "command": "speak",
            "speaker": "Ai",
            "line": "What do you mean?",
            "voice_idx": "8",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I mean, all the patients seemed to know you. I was shocked.",
            "voice_idx": null,
