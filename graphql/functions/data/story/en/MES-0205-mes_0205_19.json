        {
            "command": "end_delay"
        },
        {
            "command": "wait_for_click"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "That's not just something anyone can do. I really think everyone around you breaks out into smiles because of your presence.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "4"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Ai",
            "line": "Man, my stocks are going through the roof right now with all of your compliments, but seriously, I'm not doing anything special.",
            "voice_idx": "12",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 4.3
