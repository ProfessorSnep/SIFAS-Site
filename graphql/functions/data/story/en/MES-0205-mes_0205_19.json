        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Not at all. I should be thanking you for bringing me along with you. I hope Misato didn't feel constrained with me there?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "eye",
            "visual_eyes": "close"
