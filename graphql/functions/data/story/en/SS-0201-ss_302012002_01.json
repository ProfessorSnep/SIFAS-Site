        },
        {
            "command": "speak",
            "speaker": "Ayumu",
            "line": "This is pretty normal for childhood friends.",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Ayumu",
            "line": "Anyway, she really likes these cookies, so we're always placing them in each other's rooms.",
            "voice_idx": "8",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
