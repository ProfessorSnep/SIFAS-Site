        },
        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "It seems the two of you are as close as ever.",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
