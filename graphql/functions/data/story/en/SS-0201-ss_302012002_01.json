        },
        {
            "command": "speak",
            "speaker": "Ayumu",
            "line": "N-No! Geez, don't tease me like that, Ai.",
            "voice_idx": "5",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
