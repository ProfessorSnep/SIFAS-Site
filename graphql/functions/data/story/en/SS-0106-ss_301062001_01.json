        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Hanamaru",
            "line": "It's a rare sight to see you look for a book on something besides demons and black magic, zura. Are you searching for a particular plant?",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
