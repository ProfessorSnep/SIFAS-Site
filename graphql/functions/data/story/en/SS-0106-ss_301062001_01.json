        {
            "command": "speak",
            "speaker": "Yoshiko",
            "line": "It's not Yoshiko, it's Yohane! Anyways, I'm looking for an encyclopedia on plants. Do you have anything?",
            "voice_idx": "4",
            "is_plus": true
