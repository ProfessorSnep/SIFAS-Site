        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Yoshiko",
            "line": "Why are you staring at me with that dim look on your face?!",
            "voice_idx": "13",
            "is_plus": false
