        },
        {
            "command": "speak",
            "speaker": "Nico",
            "line": "What, got a problem?",
            "voice_idx": "14",
            "is_plus": false
        },
        {
            "command": "set_label",
            "label_value": "muse @maki"
        },
        {
            "command": "speak",
            "speaker": "Maki",
            "line": "Nope, it looks pretty good on you.",
            "voice_idx": "15",
            "is_plus": false
        },
        {
            "command": "set_label",
            "label_value": "aqours @dia"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "Indeed. They're cute, like the clothes little kids wear for Shichi-Go-San.",
            "voice_idx": "16",
            "is_plus": false
        },
