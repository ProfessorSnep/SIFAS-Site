        {
            "command": "speak",
            "speaker": "Shioriko",
            "line": "Thanks to everyone's help, I did get inspired.",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Really? That's good.",
