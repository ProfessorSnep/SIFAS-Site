        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "Wait, that's just the usual, cutesy Kasumin! C'mon, you gotta take this makeover seriously, Karin!",
            "voice_idx": "13",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
