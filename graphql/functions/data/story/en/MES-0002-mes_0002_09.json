        {
            "command": "speak",
            "speaker": "Eli",
            "line": "For my solo live show... First, I'd like to reassess myself.",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Reassess yourself?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
