        {
            "command": "speak",
            "speaker": "Umi",
            "line": "There's something I want to ask you. Did my facial expression change while I was writing?",
            "voice_idx": "12",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "What? Umm... Yeah. (A lot, if I recall correctly.)",
            "voice_idx": null,
            "is_plus": false
