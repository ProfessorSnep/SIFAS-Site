        {
            "command": "speak",
            "speaker": "Eli",
            "line": "Huh? Wh-What?!",
            "voice_idx": "13",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "When I saw you, I noticed you were walking kinda hunched over. Then, when I called your name, you jumped up and shrieked.",
