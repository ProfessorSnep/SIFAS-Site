        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Chika",
            "line": "...Oh, no!",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "character",
