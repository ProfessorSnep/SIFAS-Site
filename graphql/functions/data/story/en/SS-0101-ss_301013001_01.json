            "visual_expression": "1"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "*Laugh* That does sound pretty interesting. It'd be nice if that were possible.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
