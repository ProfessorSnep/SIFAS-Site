        {
            "command": "speak",
            "speaker": "Ruby",
            "line": "*Munch* *Gulp* Th-Thank you, Emma.",
            "voice_idx": "3",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
