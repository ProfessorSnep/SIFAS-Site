            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Ruby",
            "line": "Me too! I usually try not to eat too many snacks, but sweet potato tarts are an exception.",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
