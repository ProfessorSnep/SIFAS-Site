            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Nico",
            "line": "There's definitely all kinds of struggles involved...But they also give me so much happiness.",
            "voice_idx": "10",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "eye",
            "visual_eyes": "open"
        },
        {
            "command": "wait",
            "duration": 0.3
