        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Nico",
            "line": "That's why I'm gonna be a great big sister to them, no matter how hard it is, or how much trouble they cause me.",
            "voice_idx": "11",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
