        {
            "command": "speak",
            "speaker": "Dia",
            "line": "When I'm in a toy shop, it really makes me reminisce about my youth.",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "4"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "This one time Ruby wanted a stuffed animal so badly she sat down and refused to move. It was quite a scene.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
