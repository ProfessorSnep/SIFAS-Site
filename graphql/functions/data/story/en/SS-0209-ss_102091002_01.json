        },
        {
            "command": "speak",
            "speaker": "Rina",
            "line": "Do I need to, like...go to a drawing class?",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
