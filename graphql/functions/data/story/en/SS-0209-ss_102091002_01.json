        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Rina",
            "line": "So for now, I want to be better at using the Rina-chan Board.",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
