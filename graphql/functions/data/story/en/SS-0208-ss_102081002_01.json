        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "Like, when people are having a tough time, I want to run up to them, give them a pat on the back and make them some cheese fondue.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "character",
