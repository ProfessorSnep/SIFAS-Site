        {
            "command": "speak",
            "speaker": "Emma",
            "line": "I want to be the type of school idol that people can look to and feel better when they're lonely, or when they're tired, or something.",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
