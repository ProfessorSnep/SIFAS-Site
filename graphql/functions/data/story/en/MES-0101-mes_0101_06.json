        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Chika",
            "line": "What?! Me? Why?",
            "voice_idx": "18",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Because you're the leader, Chika. I figure they'd totally pick you to be the face of the group.",
