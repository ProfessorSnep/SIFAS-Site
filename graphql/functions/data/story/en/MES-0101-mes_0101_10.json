        {
            "command": "speak",
            "speaker": "Chika",
            "line": "Well... It's that I love singing with everyone in Aqours more than anything!",
            "voice_idx": "18",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "4",
