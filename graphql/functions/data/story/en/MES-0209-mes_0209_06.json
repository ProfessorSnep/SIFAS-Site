        {
            "command": "speak",
            "speaker": "Rina",
            "line": "I wanna do it, so I'm gonna.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "But Rina...",
