        },
        {
            "command": "speak",
            "speaker": "Rina",
            "line": "Everyone in the school! Rina-chan Board says, \"Wow!\"",
            "voice_idx": "11",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
