        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "You're working so hard, but what am I doing? I'll go get you an audience!",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
