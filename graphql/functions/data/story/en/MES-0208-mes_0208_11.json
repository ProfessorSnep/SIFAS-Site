        {
            "command": "speak",
            "speaker": "Emma",
            "line": "Thank you. I love this drink you make. After our break, I'll get back up there!",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Hey... Why don't we go somewhere else?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "hide"
        },
        {
            "command": "character",
            "character_id": "1",
