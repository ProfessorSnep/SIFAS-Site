        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "No... Every time we go, we do get a little bit closer. We just need to work a little harder for him.",
            "voice_idx": "7",
            "is_plus": true
        },
