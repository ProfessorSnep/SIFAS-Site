        },
        {
            "command": "speak",
            "speaker": "Emma",
            "line": "Hey there, what are you doing here?",
            "voice_idx": "12",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
