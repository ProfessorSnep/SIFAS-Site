            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Mari",
            "line": "I have a really happy suggestion! Let's conduct a School Idol Train!",
            "voice_idx": "5",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "5"
        },
        {
            "command": "character",
