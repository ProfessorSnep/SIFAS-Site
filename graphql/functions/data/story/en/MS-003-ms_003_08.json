        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Sure, don't mind if I do.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "*Munch* Mm! This is yakisoba noodles! With fried egg on top!",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
