        {
            "command": "speak",
            "speaker": "Umi",
            "line": "You're curious about how I spend my weekends?",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "eye",
            "visual_eyes": "close"
