        {
            "command": "wait",
            "duration": 0.3
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Do you think I'll be able to write again if I listen to the sound of the ocean too?",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "5"
        },
        {
            "command": "character",
            "character_id": "1",
