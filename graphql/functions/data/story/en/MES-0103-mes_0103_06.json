        },
        {
            "command": "speak",
            "speaker": "Kanan",
            "line": "Wh-What is it?",
            "voice_idx": "19",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "4"
