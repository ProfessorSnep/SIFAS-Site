            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "You",
            "line": "Oh, I know how you feel! The ocean is so blue, beautiful and shiny that it makes you want to salute it!",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Salute the ocean?",
            "voice_idx": null,
            "is_plus": false
        },
        {
