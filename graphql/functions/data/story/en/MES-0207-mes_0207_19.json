        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "Is Shioriko's opinion correct? Is my way of thinking self-righteous?",
            "voice_idx": "30",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
