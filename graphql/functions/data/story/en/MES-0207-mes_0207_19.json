            "command": "wait_for_click"
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "You read every single word of everyone's letters, right? Your eyes sparkled over what everyone loved, over the books and anime they recommended to you.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "But was there anything that actually swept your heart away?",
            "voice_idx": null,
            "is_plus": false
        },
