            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "I may not look it, but I can be greedy. I want everyone out there charging me up all day, every day!",
            "voice_idx": "25",
            "is_plus": false
