        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kanata",
            "line": "Of course, Haruka motivates me, but you played a huge part in it, too. Don't you believe me?",
            "voice_idx": "13",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I mean, I... Well, I hope I did.",
