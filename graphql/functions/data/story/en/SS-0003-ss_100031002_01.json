        {
            "command": "speak",
            "speaker": "Kotori",
            "line": "Sewing is my hobby, so I'm the one who designs and sews up all of Î¼'s' costumes.",
            "voice_idx": "2",
            "is_plus": false
        },
        {
            "command": "character",
