        },
        {
            "command": "speak",
            "speaker": "Kotori",
            "line": "Fluffy, full panniers, embroidered lace. I just love putting my favorite stuff into the costumes!",
            "voice_idx": "3",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kotori",
            "line": "I just want to bring out everyone's cuteness even more with the power of my costumes!",
            "voice_idx": "4",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "5"
