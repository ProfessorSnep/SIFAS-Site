        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "jump"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Mari",
            "line": "Uwah?",
