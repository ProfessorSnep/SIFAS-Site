        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Mari",
            "line": "O-Oh, right, I was supposed to give a speech, too, right? I'd totally forgotten!",
            "voice_idx": "20",
            "is_plus": false
