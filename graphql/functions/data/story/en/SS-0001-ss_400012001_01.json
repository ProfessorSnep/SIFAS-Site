        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Dia",
            "line": "That's great to hear. It's very encouraging.",
            "voice_idx": "8",
            "is_plus": false
