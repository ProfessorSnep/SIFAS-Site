        {
            "command": "speak",
            "speaker": "Hanayo",
            "line": "Huh?! Wh-Where did that come from?",
            "voice_idx": "12",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I'm sure you've gotten nervous or insecure about shows in the past too, but you always seemed to enjoy every time.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
