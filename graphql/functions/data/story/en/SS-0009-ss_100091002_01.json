        },
        {
            "command": "speak",
            "speaker": "Nico",
            "line": "It's so easy to give up, after all.",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
