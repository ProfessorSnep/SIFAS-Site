            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Riko",
            "line": "I really like drawing, and I like handicrafts, too.",
            "voice_idx": "3",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "5"
