        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Umi",
            "line": "What don't I need?",
            "voice_idx": "16",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "I found a place that sells delicious shaved ice. They're apparently special bowls of shaved ice that are only available during this season.",
            "voice_idx": null,
            "is_plus": false
