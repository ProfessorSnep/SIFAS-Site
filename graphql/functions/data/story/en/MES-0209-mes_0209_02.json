        },
        {
            "command": "speak",
            "speaker": "[player]",
            "line": "Was that your friend? It seemed like you were helping her.",
            "voice_idx": null,
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Rina",
            "line": "She's from a different class, but she returned the dictionary I let her borrow. She has some work in her next class so I told her to do her best.",
            "voice_idx": "5",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.2
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "nod",
            "character_args": [
                "-10"
            ]
        },
        {
            "command": "end_delay"
        },
        {
            "command": "wait_for_click"
        },
        {
