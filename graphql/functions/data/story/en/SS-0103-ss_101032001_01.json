        {
            "command": "set_label",
            "label_value": "aqours @you"
        },
        {
            "command": "speak",
            "speaker": "You",
            "line": "Then we had lunch there and all took a nap together. It was evening when we woke up!",
            "voice_idx": "19",
            "is_plus": false
