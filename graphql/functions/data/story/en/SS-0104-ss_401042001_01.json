        {
            "command": "speak",
            "speaker": "Dia",
            "line": "Exactly! We are one and the same. *Chuckle* This makes me feel somewhat happy.",
            "voice_idx": "17",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "2"
