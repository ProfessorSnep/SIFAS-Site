        },
        {
            "command": "speak",
            "speaker": "Ayumu",
            "line": "I realized that today when I met my fans. They all have so much faith in me.",
            "voice_idx": "6",
            "is_plus": false
        },
        {
            "command": "unknown_soundstop",
