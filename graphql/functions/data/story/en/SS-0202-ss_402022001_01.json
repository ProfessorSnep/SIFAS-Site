        },
        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "I couldn't be any more jealous!",
            "voice_idx": "16",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
            "visual_expression": "1"
        },
