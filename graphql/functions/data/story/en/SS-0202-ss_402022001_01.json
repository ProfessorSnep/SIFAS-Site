        },
        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "Kasumi had done some modeling a few days ago, so we were talking about that.",
            "voice_idx": "12",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
