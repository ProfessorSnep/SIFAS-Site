        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "I was just so excited that I was going to get my picture taken that I didn't have time to think of that!",
            "voice_idx": "8",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
