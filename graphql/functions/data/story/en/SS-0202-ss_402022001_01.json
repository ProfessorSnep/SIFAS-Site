        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Karin",
            "line": "I haven't seen how the photos turned out so I can't say anything, but I have a feeling you've gotten a little stronger-looking.",
            "voice_idx": "23",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
