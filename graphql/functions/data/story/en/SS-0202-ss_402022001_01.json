        {
            "command": "wait",
            "duration": 0.2
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Kasumi",
            "line": "R-Really? Huh, I didn't even think of it like that.",
            "voice_idx": "7",
            "is_plus": true
        },
        {
            "command": "delay",
            "duration": 0.6
        },
        {
