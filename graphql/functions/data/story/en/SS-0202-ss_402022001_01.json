        },
        {
            "command": "speak",
            "speaker": "Setsuna",
            "line": "I'd imagine so.",
            "voice_idx": "9",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "2",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
