        },
        {
            "command": "speak",
            "speaker": "Karin",
            "line": "Oh my, modeling?",
            "voice_idx": "13",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
            "visual_expression": "1"
