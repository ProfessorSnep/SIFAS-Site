        {
            "command": "wait",
            "duration": 0.5
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Umi",
            "line": "Oh, no! I guess you can say Honoka dragged me into it. I barely knew school idols existed before that.",
            "voice_idx": "5",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
