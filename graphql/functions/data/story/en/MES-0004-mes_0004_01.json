            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Umi",
            "line": "Honoka dreams of getting the whole school excited through being a school idol. I want that dream of hers to come true.",
            "voice_idx": "7",
            "is_plus": false
        },
        {
            "command": "speak",
            "speaker": "[player]",
