        {
            "command": "speak",
            "speaker": "Kanan",
            "line": "I know I said fortune-telling is pretty hit-or-miss earlier, but your reading was actually right!",
            "voice_idx": "1",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "2"
        },
        {
            "command": "character",
