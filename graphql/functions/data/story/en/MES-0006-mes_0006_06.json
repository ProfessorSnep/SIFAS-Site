        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Maki",
            "line": "I used to think I'd just do it all by myself. That I COULD do it all by myself.",
            "voice_idx": "7",
            "is_plus": false
