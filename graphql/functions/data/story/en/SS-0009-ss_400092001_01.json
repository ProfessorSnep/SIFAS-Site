        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Nico",
            "line": "I'm the oldest one there, so I've got to show them how a professional works. That's why I can't skimp out of pose practice.",
            "voice_idx": "12",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "3",
