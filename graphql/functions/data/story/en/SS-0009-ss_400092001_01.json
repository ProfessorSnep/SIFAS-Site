        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "face",
            "visual_expression": "1"
        },
        {
            "command": "character",
            "character_id": "3",
            "character_cmd": "talk"
        },
        {
            "command": "speak",
            "speaker": "Nozomi",
            "line": "I'll have to make that a family treasure. You'll be with girls from Aqours and Nijigasaki, right?",
            "voice_idx": "10",
            "is_plus": false
        },
        {
            "command": "character",
            "character_id": "1",
            "character_cmd": "face",
            "visual_expression": "1"
